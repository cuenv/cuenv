{
  "name": "cuenv-docs",
  "version": "0.1.0", 
  "description": "Documentation site for cuenv",
  "scripts": {
    "docs:build": "antora antora-playbook.yml",
    "docs:serve": "http-server build/site -p 8080 -o",
    "docs:dev": "npm run docs:build && npm run docs:serve",
    "docs:preview": "antora --fetch antora-playbook.yml && npm run docs:serve"
  },
  "devDependencies": {
    "@antora/cli": "^3.1.7",
    "@antora/site-generator": "^3.1.7",
    "@asciidoctor/tabs": "^1.0.0-beta.6",
    "http-server": "^14.1.1"
  },
  "keywords": [
    "documentation",
    "antora", 
    "asciidoc",
    "cuenv"
  ],
  "author": "cuenv Contributors",
  "license": "AGPL-3.0-or-later"
}