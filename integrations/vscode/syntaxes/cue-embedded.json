{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "CUE Embedded Languages",
  "scopeName": "cuenv.embedded.injection",
  "injectionSelector": "L:source.cue",
  "patterns": [
    { "include": "#typescript-schema" },
    { "include": "#typescript-hint" },
    { "include": "#javascript-schema" },
    { "include": "#javascript-hint" },
    { "include": "#json-schema" },
    { "include": "#json-hint" },
    { "include": "#jsonc-schema" },
    { "include": "#jsonc-hint" },
    { "include": "#yaml-schema" },
    { "include": "#yaml-hint" },
    { "include": "#shell-schema" },
    { "include": "#shell-hint" },
    { "include": "#dockerfile-schema" },
    { "include": "#dockerfile-hint" },
    { "include": "#python-schema" },
    { "include": "#python-hint" },
    { "include": "#go-schema" },
    { "include": "#go-hint" },
    { "include": "#rust-schema" },
    { "include": "#rust-hint" },
    { "include": "#markdown-schema" },
    { "include": "#markdown-hint" },
    { "include": "#nix-schema" },
    { "include": "#nix-hint" },
    { "include": "#sql-hint" }
