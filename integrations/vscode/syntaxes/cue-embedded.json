  "repository": {
    "cue-interpolation": {
      "name": "meta.interpolation.cue",
      "begin": "\\\\\\(",
      "beginCaptures": {
        "0": {
          "name": "punctuation.section.interpolation.begin.cue keyword.operator.interpolation.cue"
        }
      },
      "end": "\\)",
      "endCaptures": {
        "0": {
          "name": "punctuation.section.interpolation.end.cue keyword.operator.interpolation.cue"
        }
      },
      "contentName": "meta.embedded.expression.cue",
      "patterns": [
        { "include": "#nested-parens" },
        { "match": "[a-zA-Z_][a-zA-Z0-9_]*", "name": "variable.other.cue" },
        { "match": "\\.", "name": "punctuation.accessor.cue" },
        { "match": "\\?", "name": "keyword.operator.ternary.cue" },
        { "match": ":", "name": "keyword.operator.ternary.cue" }
      ]
    },
    "nested-parens": {
      "begin": "\\(",
      "end": "\\)",
      "patterns": [
        { "include": "#nested-parens" },
        { "match": "[a-zA-Z_][a-zA-Z0-9_]*", "name": "variable.other.cue" }
      ]
    },

    "typescript-content": {
      "begin": "(content)\\s*(:)\\s*(\"\"\")",
      "beginCaptures": {
        "1": { "name": "variable.other.property.cue" },
        "2": { "name": "punctuation.separator.key-value.cue" },
        "3": { "name": "punctuation.definition.string.begin.cue" }
      },
      "end": "\"\"\"",
      "endCaptures": {
        "0": { "name": "punctuation.definition.string.end.cue" }
      },
      "contentName": "meta.embedded.block.typescript",
      "patterns": [
        { "include": "#cue-interpolation" },
        { "include": "source.ts" }
      ]
    },

    "typescript-schema": {
      "begin": "(#TypeScript)\\s*(&?)\\s*(\\{)",
      "beginCaptures": {
        "1": { "name": "entity.name.type.cue" },
        "2": { "name": "keyword.operator.cue" },
        "3": { "name": "punctuation.definition.block.cue" }
      },
      "end": "(?=\\})",
      "patterns": [{ "include": "#typescript-content" }]
    },

    "typescript-hint": {
      "begin": "(_language|language)\\s*(:)\\s*(\"(?:typescript|ts)\")",
      "beginCaptures": {
        "1": { "name": "variable.other.property.cue" },
        "2": { "name": "punctuation.separator.key-value.cue" },
        "3": { "name": "string.quoted.double.cue" }
      },
      "end": "^\\s*\\}",
      "patterns": [{ "include": "#typescript-content" }]
    },

