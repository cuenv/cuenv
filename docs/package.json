    "preview": "antora --fetch ../antora-playbook.yml && bun run serve"
  },
  "devDependencies": {
    "@antora/cli": "^3.1.7",
    "@antora/site-generator": "^3.1.7",
    "@asciidoctor/tabs": "^1.0.0-beta.6"
  },
  "keywords": [
    "documentation",
    "antora", 
    "asciidoc",
    "cuenv"
  ],
